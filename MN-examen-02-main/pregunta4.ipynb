   "source": [
    "from scipy import optimize\n",
    "\n",
    "def f(x):\n",
    "    return x**5 + 5.25*x**4 + 4.125*x**3 - 9.125*x**2 - 14.626*x -5.625\n",
    "\n",
    "def derif(x):\n",
    "    return 5*x**4 + 21*x**3 + 12.375*x**2 - 18.25*x \n",
    "\n",
    "\n",
    "raiz= optimize.newton(f,x0=0, fprime=derif)\n",
    "print(raiz)"
   ]
